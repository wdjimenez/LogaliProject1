<mvc:View controllerName="logali.Customers.controller.App" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
	<Shell>
		<App class="myCustomCss">
			<pages>
				<Page title="{i18n>homePageTitle}">
					<headerContent>
						<Button id="helloDialogHeader" icon="sap-icon://action"  press="onShowDialog" class="sapUiSmallMarginEnd"/>
					</headerContent>
					<content>
						<mvc:XMLView viewName="logali.Customers.view.HelloPanel" />
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>